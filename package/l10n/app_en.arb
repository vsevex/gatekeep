    "description": "Position in queue message",
    "placeholders": {
      "position": {
        "type": "int"
      }
    }
  },
  "estimatedWait": "Estimated wait: {duration}",
  "@estimatedWait": {
    "description": "Estimated wait time message",
    "placeholders": {
      "duration": {
        "type": "String"
      }
    }
